{
  "compilerOptions": {
    "outDir": "./built",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "components/*": [
        "src/components/*"
      ]
    },
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  }
}